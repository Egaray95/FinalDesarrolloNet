@model IEnumerable<ClinicaP2.Models.HistClinico>

<h2>@ViewBag.name</h2>

<table class="table">
    <tr>
        <th>
            ESPECIALIDAD
        </th>
        <th>
            SINTOMAS
        </th>
        <th>
            TRATAMIENTO
        </th>
        <th>
            OBSERVACIONES
        </th>
        <th>
            DOCTOR
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Consulta.Trabajador.Especialidad)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Histsintomas)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HistTratamiento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HistObservaciones)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Consulta.Trabajador.Persona.PerNombre)
    </td>
</tr>
}

</table>
